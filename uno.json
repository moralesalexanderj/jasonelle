{
  "$jason": {
    "head": {
      "title": {"MMenú"
        type: "text"
        
      },
      "styles": {
        "bold": {
          "font": "HelveticaNeue-CondensedBold",
          "padding": "10",
          "size": "15"
        },
        "normal": {
          "font": "HelveticaNeue",
          "size": "12"
        },
        "background": {
          "image": "https://lh3.googleusercontent.com/p/AF1QipMzOlyQKT72nb8O_-Wh7CNXtSJZduZF5LXdBnG8=w768-h768-n-o-v1"
        }
      },
      "actions": {
        "$foreground": {
          "type": "$reload"
        },
        "send": {
          "type": "$util.alert",
          "options": {
            "title": "Donalicious",
            "description": "Iniciando Búsqueda de {{$get.message}} en nuestro Catálogo."
          }
        }
      }
    },
    "body": {
      "header": {
        "menu": {
          "text": "Donalicious.com",
          "href": {
            "url": "https:/www.donalicious.com",
            "view": "web"
          },
          "size": 20
        }
      },
      "footer": {
        "input": {
          "left": {
            "image": "https://xn--malamaa-9za.com.ve/wp-content/uploads/2020/02/mln-logo-principal-web.png",
            "action": {
              "type": "$media.camera"
            }
          },
          "name": "message",
          "placeholder": "Palabra clave...",
          "right": {
            "text": "Buscar",
            "action": {
              "trigger": "send"
            }
          }
        }
      }
    }
  }
}